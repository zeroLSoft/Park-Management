            <Label layoutX="69.0" layoutY="22.0" text="Monthly park reports">
               <font>
                  <Font name="System Bold Italic" size="42.0" />
               </font>
            </Label>
            <Button fx:id="btnProfit" layoutX="354.0" layoutY="162.0" mnemonicParsing="false" onAction="#getViewProfit" text="Profit Report" />
