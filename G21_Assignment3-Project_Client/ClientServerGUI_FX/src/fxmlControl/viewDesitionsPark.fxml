            <Label layoutX="214.0" layoutY="30.0" text="Decision History">
               <font>
                  <Font name="System Bold Italic" size="43.0" />
               </font>
            </Label>
            <TableView fx:id="DesitionsTable" layoutX="66.0" layoutY="118.0" prefHeight="249.0" prefWidth="622.0">
              <columns>
                <TableColumn fx:id="descriptionCol" prefWidth="321.0" text="Description" />
                <TableColumn fx:id="beforCol" prefWidth="93.0" text="Befor" />
                  <TableColumn fx:id="afterCol" prefWidth="86.0" text="After" />
                  <TableColumn fx:id="statusCol" prefWidth="121.0" text="Status" />
              </columns>
            </TableView>
            <Button fx:id="btnReturn" layoutX="602.0" layoutY="393.0" mnemonicParsing="false" onAction="#getReturnBtn" text="Return" />
         </children>
      </Pane>
   </children>
</AnchorPane>
